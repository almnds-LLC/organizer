{
  "report-dir": "coverage-merged",
  "temp-dir": ".nyc_output",
  "reporter": ["text", "lcov", "html"],
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["src/__tests__/**", "**/*.d.ts", "**/node_modules/**"],
  "check-coverage": true,
  "statements": 35,
  "branches": 35,
  "functions": 20,
  "lines": 30
}
